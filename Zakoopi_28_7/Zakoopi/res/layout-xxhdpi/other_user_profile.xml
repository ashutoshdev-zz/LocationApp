                            android:layout_height="fill_parent"
                            android:layout_marginBottom="10.67dp"
                            android:layout_marginTop="10.67dp"
                            android:background="#CBCBC9" />

                        <RelativeLayout
                            android:id="@+id/rel_look"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center" >

                            <LinearLayout
                                android:id="@+id/lin_look"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_marginBottom="5.67dp"
                                android:layout_marginTop="5.67dp"
                                android:orientation="vertical"
                                android:weightSum="3" >

                                <RelativeLayout
                                    android:id="@+id/rel_lookbook"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1" >

                                    <TextView
                                        android:id="@+id/txt_lookbook_count"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="30"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />

                                    <TextView
                                        android:id="@+id/txt_lookbook"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="7.67dp"
                                        android:layout_toRightOf="@+id/txt_lookbook_count"
                                        android:text="Lookbook"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rel_review"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1" >

                                    <TextView
                                        android:id="@+id/txt_review_count"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="30"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />

                                    <TextView
                                        android:id="@+id/txt_review"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="7.67dp"
                                        android:layout_toRightOf="@+id/txt_review_count"
                                        android:text="Review"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rel_like"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1" >

                                    <TextView
                                        android:id="@+id/txt_like_count"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="30"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />

                                    <TextView
                                        android:id="@+id/txt_like"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="7.67dp"
                                        android:layout_toRightOf="@+id/txt_like_count"
                                        android:text="Like"
                                        android:textColor="#FFF"
                                        android:textSize="15dp" />
                                </RelativeLayout>
                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
