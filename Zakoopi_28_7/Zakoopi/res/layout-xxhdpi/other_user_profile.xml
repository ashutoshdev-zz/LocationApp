            <RelativeLayout
                android:id="@+id/rel_follow"
                android:layout_width="fill_parent"
                android:layout_height="46.00dp"
                android:layout_marginBottom="11.00dp"
                android:layout_marginLeft="20.67dp"
                android:layout_marginTop="11.00dp"
                android:background="#FFFFFF" >

                <ImageView
                    android:id="@+id/img_follow"
                    android:layout_width="21.00dp"
                    android:layout_height="24.67dp"
