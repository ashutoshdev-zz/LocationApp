<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:feedpage="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e7e7e7" >

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="#e7e7e7" />

    <com.zakoopi.helper.RecyclerViewHeader
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="390.67dp"
        android:layout_gravity="center_horizontal|top" >

        <RelativeLayout
            android:id="@+id/rel_main"
            android:layout_width="fill_parent"
            android:layout_height="302.67dp"
            android:background="@drawable/background_seekbar" >

            <RelativeLayout
                android:id="@+id/rel_layout"
                android:layout_width="fill_parent"
                android:layout_height="257.00dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10.67dp"
                android:layout_marginRight="10.67dp"
                android:layout_marginTop="24.00dp" >

                <RelativeLayout
                    android:id="@+id/rel_back_white"
                    android:layout_width="fill_parent"
                    android:layout_height="178.33dp"
                    android:background="@drawable/profile_white_drawable" >

                    <RelativeLayout
                        android:id="@+id/rel_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true" >

                        <ImageView
                            android:id="@+id/img_profile"
                            android:layout_width="67.67dp"
                            android:layout_height="67.67dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dp"
                            android:src="@drawable/home_profile_pic" />

                        <TextView
                            android:id="@+id/txt_user_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/img_profile"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="8dp"
                            android:text="User Name"
                            android:textColor="#4d4d49"
                            android:textSize="18.33dp" />

                        <LinearLayout
                            android:id="@+id/lin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/txt_user_name"
                            android:layout_marginTop="1dp"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="3" >

                            <TextView
                                android:id="@+id/txt_user_age"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="26"
                                android:textColor="#898989"
                                android:textSize="15dp" />

                            <View
                                android:id="@+id/v1"
                                android:layout_width="5dp"
                                android:layout_height="5dp"
                                android:layout_marginLeft="3dp"
                                android:background="@drawable/dot" />

                            <TextView
                                android:id="@+id/txt_user_gander"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:text="Male"
                                android:textColor="#898989"
                                android:textSize="15dp" />

                            <View
                                android:id="@+id/v2"
                                android:layout_width="5dp"
                                android:layout_height="5dp"
                                android:layout_marginLeft="3dp"
                                android:background="@drawable/dot" />

                            <TextView
                                android:id="@+id/txt_user_location"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="1"
                                android:text="New delhi"
                                android:textColor="#898989"
                                android:textSize="15dp" />
                        </LinearLayout>

                        <RelativeLayout
                            android:id="@+id/rel_social"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/lin"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="8dp"
                            android:layout_marginTop="10.67dp"
                            android:gravity="center|center_horizontal" >

                            <LinearLayout
                                android:id="@+id/lin1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal"
                                android:weightSum="4" >

                                <ImageView
                                    android:id="@+id/img_facebook"
                                    android:layout_width="26.05dp"
                                    android:layout_height="26.05dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/profile_fb_icon" />

                                <View
                                    android:id="@+id/view"
                                    android:layout_width="10.67dp"
                                    android:layout_height="3dp" />

                                <ImageView
                                    android:id="@+id/img_twitter"
                                    android:layout_width="26.05dp"
                                    android:layout_height="26.05dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/profile_twitter_icon" />

                                <View
                                    android:id="@+id/view1"
                                    android:layout_width="10.67dp"
                                    android:layout_height="3dp" />

                                <ImageView
                                    android:id="@+id/img_website"
                                    android:layout_width="26.05dp"
                                    android:layout_height="26.05dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/profile_web_icon" />

                                <View
                                    android:id="@+id/view2"
                                    android:layout_width="10.67dp"
                                    android:layout_height="3dp" />

                                <ImageView
                                    android:id="@+id/img_more"
                                    android:layout_width="4.33dp"
                                    android:layout_height="15.67dp"
                                    android:layout_weight="1"
                                    android:src="@drawable/profile_more_icon" />
                            </LinearLayout>
                        </RelativeLayout>
                    </RelativeLayout>

                    <!--
                         <ImageView
                        android:id="@+id/img_edt_profile"
                        android:layout_width="28dp"
                        android:layout_height="28dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/rel_detail"
                        android:layout_marginRight="11.33dp"
                        android:layout_marginTop="10.67dp"
                        android:src="@drawable/profile_edit_icon" />
                    -->
